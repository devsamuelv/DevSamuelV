	"Lua.diagnostics.disable": [
		"unused-local",
		"lowercase-global",
		"unused-function"
	],
	"Lua.diagnostics.severity": {
		"redefined-local": "Warning"
	},
	"Lua.diagnostics.globals": [
		"printError",
		"sleep",
		"read",
		"write",
		"print",
		"colours",
		"colors",
		"commands",
		"disk",
		"fs",
		"gps",
		"help",
		"http",
		"paintutils",
		"parallel",
		"peripheral",
		"rednet",
		"redstone",
		"keys",
		"settings",
		"shell",
		"multishell",
		"term",
		"textutils",
		"turtle",
		"pocket",
		"vector",
		"bit32",
		"window",
		"_CC_DEFAULT_SETTINGS",
		"_HOST",
		"_VERSION",
		"_"
	],
	"Lua.runtime.version": "Lua 5.1",

	"editor.formatOnSave": true,
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/.next/**": true,
		"**/.next/cache/**": true,
		"**/.next/server/**": true,
		"**/.next/static/**": true,
		"**/node_modules/*/**": true,
		"**/yarn-error.txt": true
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[go]": {
		"editor.insertSpaces": false,
		"editor.formatOnSave": true,
		"editor.codeActionsOnSave": {
			"source.organizeImports": true
		}
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[java]": {
		"editor.suggest.snippetsPreventQuickSuggestions": false
	},
	"javascript.updateImportsOnFileMove.enabled": "always",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"search.exclude": {
		"**/next": true,
		"**/node_modules": false,
		"**/node_modules/**": true
	},
	"files.exclude": {
		"**/node_modules/**": true,
		"**/tags": true,
		"**/yarn.lock": true,
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	},
	"python.languageServer": "Pylance",
	"diffEditor.ignoreTrimWhitespace": true,
	"workbench.preferredLightColorTheme": "Simple Dark",
	"workbench.preferredDarkColorTheme": "Arkham Theme",
	"editor.quickSuggestionsDelay": 1,
	"editor.suggestLineHeight": 28,
	"editor.tabSize": 2,
	"terminal.integrated.fontFamily": "JetBrains Mono NL",
	"prettier.useTabs": true,
	"workbench.productIconTheme": "fluent-icons",
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter-notebook"
	},
	"codestream.showMarkerGlyphs": true,
	"codestream.showInStatusBar": "left",
	"codestream.showMarkerCodeLens": true,
	"terminal.integrated.fontWeight": "normal",
	"maven.executable.path": "~/Documents/apache-maven-3.6.3/bin",
	"java.completion.importOrder": ["java", "javax"],
	"java.refactor.renameFromFileExplorer": "preview",
	"go.toolsManagement.autoUpdate": true,
	"[prisma]": {
		"editor.minimap.enabled": false
	},
	"cryptoticker.cryptoSymbols": ["BTC"],
	"editor.fontSize": 12,
	"redhat.telemetry.enabled": true,
	"terminal.integrated.tabs.enabled": true,
	"terminal.integrated.defaultProfile.linux": "zsh",
	"rojo.pluginManagement": true,
	"java.project.importOnFirstTimeStartup": "automatic",
	"omnisharp.useGlobalMono": "always",
	"vs-kubernetes": {
		"vscode-kubernetes.helm-path.linux": "/home/samuelvillegas/.vs-kubernetes/tools/helm/linux-amd64/helm",
