		"vscode-kubernetes.kubectl-path.linux": "/home/samuelvillegas/.vs-kubernetes/tools/kubectl/kubectl"
	},
	"qttools.searchMode": "path",
	"editor.cursorBlinking": "smooth",
	"oneDarkPro.vivid": false,
	"oneDarkPro.italic": true,
